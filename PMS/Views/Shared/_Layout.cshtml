    <!-- Custom js -->
    <script type="text/javascript" src="~/admin_assets/pages/dashboard/custom-dashboard.min.js"></script>
    <script type="text/javascript" src="~/admin_assets/js/script.js"></script>
    <script type="text/javascript " src="~/admin_assets/js/SmoothScroll.js"></script>
    <script src="~/admin_assets/js/pcoded.min.js"></script>

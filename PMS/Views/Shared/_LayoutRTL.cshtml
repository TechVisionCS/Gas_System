    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    <meta name="author" content="">
    <!-- Favicon icon -->
    <link rel="icon" href="~/assets/images/logo5.png" type="image/x-icon">
    <link rel="stylesheet" href="~/wwwroot/admin_asset_rtl/css/fontfamily/bahij_halvetic_roman.ttf" />
    <link rel="stylesheet" href="~/wwwroot/admin_asset_rtl/css/fontfamily/bahij_halvetic_bold.ttf" />
    <link rel="stylesheet" href="~/wwwroot/admin_asset_rtl/css/fontfamily/bahij_halvetic_light.ttf" />
    <!-- Google font-->
    <style>
        @@font-face {
            font-family: "Montserrat", "Bahij Halvetica", "Segoe UI" Arial, sans-serif !important;
        }

        input[type=text] {
            direction: rtl;
        }


        /* OR */

        .table thead,
        .table th {
            text-align: right;
            direction: rtl;
        }

        table thead,
        .table td {
            text-align: right;
            direction: rtl;
        }
    </style>
    <!-- Required Fremwork -->
    <link href="~/admin_asset_rtl/css/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/admin_asset_rtl/css/bootstrap/css/bootstrap.min.css">
    <!-- themify-icons line icon -->

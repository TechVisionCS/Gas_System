    <link rel="stylesheet" type="text/css" href="~/admin_asset_rtl/icon/font-awesome/css/font-awesome.min.css">
    <!-- ico font -->
    <link rel="stylesheet" type="text/css" href="~/admin_asset_rtl/icon/icofont/css/icofont.css">

    <!-- DataTables -->
    <link rel="stylesheet" href="~/lib/datatables-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="~/lib/datatables-responsive/css/responsive.bootstrap4.min.css">
    <link rel="stylesheet" href="~/lib/datatables-buttons/css/buttons.bootstrap4.min.css">

    <!-- Select2 -->
    <link rel="stylesheet" href="~/lib/select2/css/select2.min.css">
    <link rel="stylesheet" href="~/lib/select2-bootstrap4-theme/select2-bootstrap4.min.css">

    <!-- Style.css -->

    <link rel="stylesheet" type="text/css" href="~/admin_asset_rtl/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/admin_asset_rtl/css/jquery.mCustomScrollbar.css">
    <link href="~/css/site.css" rel="stylesheet" />

    @RenderSection("Styles", required: false)
</head>

<body style="" dir="rtl">
    @*<div class="fixed-button">
            <a href="https://codedthemes.com/item/gradient-able-admin-template" target="_blank" class="btn btn-md btn-primary">
                <i class="fa fa-shopping-cart" aria-hidden="true"></i> Upgrade To Pro
            </a>
        </div>*@
    <!-- Pre-loader start -->
    <div class="theme-loader">
        <div class="loader-track">
            <div class="loader-bar"></div>
        </div>
    </div>
    <!-- Pre-loader end -->
    <div id="pcoded" class="pcoded">
        <div class="pcoded-overlay-box"></div>
        <div class="pcoded-container navbar-wrapper">

            @await Html.PartialAsync("_HeaderPartial.cshtml")

            <div class="pcoded-main-container">
                <div class="pcoded-wrapper">

                    @await Html.PartialAsync("_LeftSidebarPartial.cshtml")

                    <div class="pcoded-content">
                        <div class="pcoded-inner-content">

                            @RenderBody()

                            <div id="styleSelector">

                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Required Jquery -->
    <script type="text/javascript" src="~/admin_asset_rtl/js/jquery/jquery.min.js"></script>
    <script type="text/javascript" src="~/admin_asset_rtl/js/jquery-ui/jquery-ui.min.js"></script>
    <script type="text/javascript" src="~/admin_asset_rtl/js/popper.js/popper.min.js"></script>
    <script type="text/javascript" src="~/admin_asset_rtl/js/bootstrap/js/bootstrap.min.js"></script>
    <!-- jquery slimscroll js -->
    <script type="text/javascript" src="~/admin_asset_rtl/jquery-slimscroll/jquery.slimscroll.js"></script>
    <!-- modernizr js -->
    <script type="text/javascript" src="~/admin_asset_rtl/js/modernizr/modernizr.js"></script>
    <!-- am chart -->
    <!--<script src="~/admin_assets/pages/widget/amchart/amcharts.min.js"></script>
